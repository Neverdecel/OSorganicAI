# CHILD_TEMPLATE Environment Variables
# TODO: Copy this to .env and fill in your actual values

# ============================================
# Application Settings
# ============================================
APP_ENV=development
DEBUG=true
LOG_LEVEL=DEBUG
LOG_FORMAT=json

# ============================================
# AI Model Configuration
# ============================================
# Options: openai, anthropic, ollama
AI_MODEL_PROVIDER=openai
AI_API_KEY=your-api-key-here

# OpenAI specific
OPENAI_API_KEY=your-openai-key-here
OPENAI_MODEL=gpt-4-1106-preview
OPENAI_TEMPERATURE=0.7

# Anthropic specific (if using)
# ANTHROPIC_API_KEY=your-anthropic-key-here
# ANTHROPIC_MODEL=claude-3-opus-20240229

# Ollama specific (if using)
# OLLAMA_BASE_URL=http://localhost:11434
# OLLAMA_MODEL=llama2

# ============================================
# GitHub Configuration
# ============================================
GITHUB_TOKEN=your-github-personal-access-token
GITHUB_REPO=your-org/your-repo
GITHUB_WEBHOOK_SECRET=your-webhook-secret

# ============================================
# Supabase Configuration
# ============================================
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# ============================================
# Domain-Specific Settings (Optional)
# ============================================
# Add any domain-specific configuration here
# Example for e-commerce:
# STRIPE_SECRET_KEY=sk_test_...
# PAYMENT_CURRENCY=usd

# Example for fintech:
# PLAID_CLIENT_ID=xxx
# PLAID_SECRET=xxx

# Example for healthcare:
# HIPAA_ENCRYPTION_KEY=xxx
